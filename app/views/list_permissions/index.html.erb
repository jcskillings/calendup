<h1>Listing list_permissions</h1>

<table>
  <thead>
    <tr>
      <th>Listid</th>
      <th>Userid</th>
      <th>Permissiontype</th>
      <th>Sharestatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @list_permissions.each do |list_permission| %>
      <tr>
        <td><%= list_permission.listId %></td>
        <td><%= list_permission.UserId %></td>
        <td><%= list_permission.permissionType %></td>
        <td><%= list_permission.shareStatus %></td>
        <td><%= link_to 'Show', list_permission %></td>
        <td><%= link_to 'Edit', edit_list_permission_path(list_permission) %></td>
        <td><%= link_to 'Destroy', list_permission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List permission', new_list_permission_path %>
